{
    "name": "horizon-site",
    "version": "0.1.0",
    "description": "Front-end of horizon-efrei.fr",
    "author": "Horizon Front-end Dev. Team",
    "private": true,
    "license": "MIT",
    "scripts": {
        "dev": "vite",
        "lint": "eslint './src/**/*.{js,vue}'",
        "lint:fix": "npm run lint -- --fix --debug",
        "prettier": "prettier './src/**/*.{js,vue}' --check",
        "prettier:fix": "prettier './src/**/*.{js,vue}' --write",
        "stylelint": "stylelint './src/**/*.{vue,css,scss}'",
        "stylelint:fix": "stylelint './src/**/*.{vue,css,scss}' --fix",
        "format": "npm run lint:fix && npm run stylelint:fix && npm run prettier:fix",
        "format:soft": "npm run lint && npm run stylelint && npm run prettier",
        "build": "vite build",
        "serve": "vite preview"
    },
    "dependencies": {
        "@vuelidate/core": "2.0.0-alpha.40",
        "@vuelidate/validators": "2.0.0-alpha.28",
        "@vueuse/core": "8.2.4",
        "axios": "0.26.1",
        "easymde": "2.16.1",
        "highlight.js": "11.5.1",
        "lodash": "4.17.21",
        "marked": "4.0.14",
        "mitt": "3.0.0",
        "nanoid": "3.3.2",
        "pinia": "2.0.13",
        "typesense-instantsearch-adapter": "2.4.0",
        "url-join": "5.0.0",
        "vue": "3.2.31",
        "vue-avatar-cropper": "6.0.9",
        "vue-i18n": "9.2.0-beta.34",
        "vue-image-crop-upload": "3.0.3",
        "vue-instantsearch": "4.3.3",
        "vue-router": "4.0.14",
        "vue3-cookies": "1.0.6",
        "vue3-popper": "1.4.2",
        "vuex": "4.0.2"
    },
    "devDependencies": {
        "@tailwindcss/aspect-ratio": "0.4.0",
        "@tailwindcss/forms": "0.5.0",
        "@tailwindcss/line-clamp": "0.3.1",
        "@tailwindcss/typography": "0.5.2",
        "@types/tailwindcss": "3.0.10",
        "@vitejs/plugin-vue": "2.3.1",
        "autoprefixer": "10.4.4",
        "eslint": "8.14.0",
        "eslint-config-prettier": "8.5.0",
        "eslint-plugin-tailwindcss": "3.5.0",
        "eslint-plugin-vue": "8.7.1",
        "postcss-html": "1.3.0",
        "prettier": "2.6.2",
        "sass": "1.49.11",
        "sass-loader": "12.6.0",
        "stylelint": "14.7.1",
        "stylelint-config-css-modules": "4.1.0",
        "stylelint-config-prettier": "9.0.3",
        "stylelint-config-recess-order": "3.0.0",
        "stylelint-config-recommended": "7.0.0",
        "stylelint-config-recommended-scss": "6.0.0",
        "stylelint-config-recommended-vue": "1.4.0",
        "stylelint-config-standard": "25.0.0",
        "stylelint-config-standard-scss": "3.0.0",
        "tailwindcss": "3.0.23",
        "vite": "2.9.1"
    }
}
